<Dialog Name="VectorStreamNetworkAnalysis" HelpFile="VectorStreamNetworkAnalysis.html">
    <DialogComponent type="DialogFile">
        <Name>Streams</Name>
        <Description>Enter the name of the Streams file here</Description>
        <LabelText>Input Vector Streams File:</LabelText>
        <DialogMode>Open File</DialogMode>
        <Filter>Vector Files (*.shp), SHP</Filter>
        <IsVisible>True</IsVisible>
        <MakeOptional>False</MakeOptional>
        <ShowButton>True</ShowButton>
    </DialogComponent>
    <DialogComponent type="DialogFile">
        <Name>DEM</Name>
        <Description>Enter the name of the DEM input raster here</Description>
        <LabelText>Input Raster DEM File:</LabelText>
        <DialogMode>Open File</DialogMode>
        <Filter>Raster Files (*.dep), DEP</Filter>
        <IsVisible>True</IsVisible>
        <MakeOptional>False</MakeOptional>
        <ShowButton>True</ShowButton>
    </DialogComponent>
    <DialogComponent type="DialogFile">
        <Name>Lakes</Name>
        <Description>Optionally enter the name of the vector lakes file (polygon ShapeType)</Description>
        <LabelText>Input Vector Lakes File (optional):</LabelText>
        <DialogMode>Open File</DialogMode>
        <Filter>Vector Files (*.shp), SHP</Filter>
        <IsVisible>True</IsVisible>
        <MakeOptional>True</MakeOptional>
        <ShowButton>True</ShowButton>
    </DialogComponent>
    <DialogComponent type="DialogFile">
        <Name>Output</Name>
        <Description>Enter the name of output streams vector here</Description>
        <LabelText>Output Vector Streams File:</LabelText>
        <DialogMode>Save File</DialogMode>
        <Filter>Vector Files (*.shp), SHP</Filter>
        <IsVisible>True</IsVisible>
        <MakeOptional>False</MakeOptional>
        <ShowButton>True</ShowButton>
    </DialogComponent>
    <DialogComponent type="DialogDataInput">
        <Name>SnapDistance</Name>
        <Description>Enter the snap distance (in meters) for joining disjoint stream links</Description>
        <LabelText>Snap Distance:</LabelText>
        <IsVisible>True</IsVisible>
        <InitialText></InitialText>
        <NumericalInputOnly>True</NumericalInputOnly>
        <MakeOptional>False</MakeOptional>
    </DialogComponent>
</Dialog>
