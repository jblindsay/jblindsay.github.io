<Dialog Name="CostPathway" HelpFile="CostPathway.html">
	<DialogComponent type="DialogFile">
		<Name>destHeader</Name>
		<Description>Enter the name of the input destination file here</Description>
		<LabelText>Input Destination Raster File:</LabelText>
		<DialogMode>Open File</DialogMode>
		<Filter>Raster Files (*.dep), DEP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>
        <DialogComponent type="DialogFile">
		<Name>backlinkHeader</Name>
		<Description>Enter the name of the back-link image here</Description>
		<LabelText>Input Back-Link Raster File:</LabelText>
		<DialogMode>Open File</DialogMode>
		<Filter>Raster Files (*.dep), DEP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>
        <DialogComponent type="DialogFile">
		<Name>outputFile</Name>
		<Description>Enter the name of the output file here</Description>
		<LabelText>Output Cost Path Raster File:</LabelText>
		<DialogMode>Save File</DialogMode>
		<Filter>Raster Files (*.dep), DEP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>
</Dialog>
