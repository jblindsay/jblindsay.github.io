<Dialog Name="Union" HelpFile="Union.html">
	<DialogComponent type="DialogMultiFile">
		<Name>inputFiles</Name>
		<Description>Enter the name of the input files here</Description>
		<LabelText>Input Vector Files:</LabelText>
		<Filter>Vector Files (*.shp), SHP</Filter>
		<IsVisible>True</IsVisible>
	</DialogComponent>
        <DialogComponent type="DialogFile">
		<Name>outputFile</Name>
		<Description>Enter the name of the output file here</Description>
		<LabelText>Output Vector File:</LabelText>
		<DialogMode>Save File</DialogMode>
		<Filter>Vector Files (*.shp), SHP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>
</Dialog>
