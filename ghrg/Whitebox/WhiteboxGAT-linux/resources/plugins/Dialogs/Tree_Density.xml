<Dialog Name="Tree_Density" HelpFile="Tree_Density.html">
	<DialogComponent type="DialogFile">
		<Name>InputFile</Name>
		<Description>Enter the name of the input file here</Description>
		<LabelText>Input LiDAR File:</LabelText>
		<DialogMode>Open File</DialogMode>
		<Filter>Raster Files (*.dep), DEP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>

        <DialogComponent type="DialogDataInput">
		<Name>suffix</Name>
		<Description>Enter any suffix that you would like attached to output file names here</Description>
		<LabelText>Output File Suffix (e.g. last return):</LabelText>
		<IsVisible>True</IsVisible>
		<InitialText></InitialText>
		<NumericalInputOnly>False</NumericalInputOnly>
		<MakeOptional>False</MakeOptional>
	</DialogComponent>


        <DialogComponent type="DialogDataInput">
		<Name>Threshold</Name>
		<Description>Enter tree minimum height</Description>
		<LabelText>Threshold Tree Height:</LabelText>
		<IsVisible>True</IsVisible>
		<InitialText>0</InitialText>
		<NumericalInputOnly>True</NumericalInputOnly>
		<MakeOptional>False</MakeOptional>
	</DialogComponent>

        <DialogComponent type="DialogDataInput">
		<Name>a</Name>
		<Description>Enter 'a' coefficient in the search algorithm (a + bh + ch^2)</Description>
		<LabelText>a:</LabelText>
		<IsVisible>True</IsVisible>
		<InitialText>2.51503</InitialText>
		<NumericalInputOnly>True</NumericalInputOnly>
		<MakeOptional>False</MakeOptional>
	</DialogComponent>

        <DialogComponent type="DialogDataInput">
		<Name>b</Name>
		<Description>Enter 'b' coefficient in the search algorithm (a + bh + ch^2)</Description>
		<LabelText>b:</LabelText>
		<IsVisible>True</IsVisible>
		<InitialText>0</InitialText>
		<NumericalInputOnly>True</NumericalInputOnly>
		<MakeOptional>False</MakeOptional>
	</DialogComponent>

        <DialogComponent type="DialogDataInput">
		<Name>c</Name>
		<Description>Enter 'c' coefficient in the search algorithm (a + bh + ch^2)</Description>
		<LabelText>c:</LabelText>
		<IsVisible>True</IsVisible>
		<InitialText>0.00901</InitialText>
		<NumericalInputOnly>True</NumericalInputOnly>
		<MakeOptional>False</MakeOptional>
	</DialogComponent>

        <DialogComponent type="DialogDataInput">
		<Name>Max</Name>
		<Description>Enter Maximum Tree Height</Description>
		<LabelText>Maximum Tree Height</LabelText>
		<IsVisible>True</IsVisible>
		<InitialText>50</InitialText>
		<NumericalInputOnly>True</NumericalInputOnly>
		<MakeOptional>False</MakeOptional>
	</DialogComponent>

        <DialogComponent type="DialogDataInput">
		<Name>Cellsize</Name>
		<Description>Enter Output Raster Cell Size</Description>
		<LabelText>Cell Size</LabelText>
		<IsVisible>True</IsVisible>
		<InitialText>1</InitialText>
		<NumericalInputOnly>True</NumericalInputOnly>
		<MakeOptional>False</MakeOptional>
	</DialogComponent>

        <DialogComponent type="DialogDataInput">
		<Name>Radius</Name>
		<Description>Enter Search Radious for Density</Description>
		<LabelText>Search Radius</LabelText>
		<IsVisible>True</IsVisible>
		<InitialText>100</InitialText>
		<NumericalInputOnly>True</NumericalInputOnly>
		<MakeOptional>False</MakeOptional>
	</DialogComponent>


</Dialog>
