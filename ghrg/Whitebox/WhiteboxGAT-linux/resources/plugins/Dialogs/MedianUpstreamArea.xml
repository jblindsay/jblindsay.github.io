<Dialog Name="FlowAccumDinf" HelpFile="FlowAccumDinf.html">
	<DialogComponent type="DialogFile">
		<Name>DEM</Name>
		<Description>Enter the name of the DEM input raster here</Description>
		<LabelText>DEM Input Raster:</LabelText>
		<DialogMode>Open File</DialogMode>
		<Filter>Raster Files (*.dep), DEP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>
	<DialogComponent type="DialogFile">
		<Name>StreamsInputFile</Name>
		<Description>Enter the name of the Streams input raster here</Description>
		<LabelText>Streams Input Raster:</LabelText>
		<DialogMode>Open File</DialogMode>
		<Filter>Raster Files (*.dep), DEP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>
	<DialogComponent type="DialogFile">
		<Name>OutputRaster</Name>
		<Description>Enter the name of the Median Upstream Area output raster here</Description>
		<LabelText>Median Upstream Area Output Raster:</LabelText>
		<DialogMode>Save File</DialogMode>
		<Filter>Raster Files (*.dep), DEP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>
</Dialog>
