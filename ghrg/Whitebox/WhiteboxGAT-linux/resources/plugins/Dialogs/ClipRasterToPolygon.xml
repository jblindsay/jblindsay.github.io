<Dialog Name="ClipRasterToPolygon" HelpFile="ClipRasterToPolygon.html">
	<DialogComponent type="DialogMultiFile">
		<Name>inputFiles</Name>
		<Description>Enter the name of the input files here</Description>
		<LabelText>Input Raster Files:</LabelText>
		<Filter>Raster Files (*.dep), DEP</Filter>
		<IsVisible>True</IsVisible>
	</DialogComponent>
	<DialogComponent type="DialogFile">
		<Name>clipFile</Name>
		<Description>Enter the name of the input vector file used for clipping here. It must be of polygon base shapetype.</Description>
		<LabelText>Input Clip File (vector polygon):</LabelText>
		<DialogMode>Open File</DialogMode>
		<Filter>ShapeFile (*.shp), SHP</Filter>
		<IsVisible>True</IsVisible>
		<MakeOptional>False</MakeOptional>
		<ShowButton>True</ShowButton>
	</DialogComponent>
        <DialogComponent type="DialogCheckBox">
		<Name>maintainInputDimensions</Name>
		<Description>Maintain the input raster dimensions?</Description>
		<LabelText>Maintain input raster dimensions?</LabelText>
		<InitialState>False</InitialState>
		<IsVisible>True</IsVisible>
	</DialogComponent>
</Dialog>
